<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.mimacom.ddd.pub.pub.diagramProvider">
      <renderer
            id="dim.typeDiagram.png"
            diagramName="Domain Information Structure (PNG)"
            diagramType="informationStructure"
            diagramRootClass="com.mimacom.ddd.dm.base.DInformationModel"
            outputFileFormat="PNG"
            rendererClass="com.mimacom.ddd.dm.dim.diagramProvider.DimTypeDiagramRendererPNGExtensionFactory:com.mimacom.ddd.dm.dim.diagramProvider.DimTypeDiagramRenderer">
      </renderer>
      <renderer
            id="dim.typeDiagram.svg"
            diagramName="Domain Information Structure (SVG)"
            diagramType="informationStructure"
            diagramRootClass="com.mimacom.ddd.dm.base.DInformationModel"
            outputFileFormat="SVG"
            rendererClass="com.mimacom.ddd.dm.dim.diagramProvider.DimTypeDiagramRendererSVGExtensionFactory:com.mimacom.ddd.dm.dim.diagramProvider.DimTypeDiagramRenderer">
      </renderer>
      <diagramType
            id="informationStructure"
            name="Information Structure">
      </diagramType>
   </extension>
   
   <extension
         point="com.mimacom.ddd.pub.pub.tableProvider">
      <renderer
            id="dim.typesAndFeaturesTable"
            tableName="All Types and Features"
            tableType="typesAndFeatures"
            tableRootClass="com.mimacom.ddd.dm.base.DInformationModel"
            rendererClass="com.mimacom.ddd.dm.dim.tableProvider.DimTypesTableRendererExtensionFactory:com.mimacom.ddd.dm.dim.tableProvider.DimTypesTableRenderer">
      </renderer>
      <diagramType
            id="typesAndFeatures"
            name="All Types and Features">
      </diagramType>
   </extension>

</plugin>
